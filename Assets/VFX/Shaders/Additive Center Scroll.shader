				float2 texCoord57 = IN.ase_texcoord3.xy * _Tililng + float2( 0,0 );
				float2 CenteredUV15_g2 = ( texCoord57 - ( _Tililng * 0.5 ) );
				float2 break17_g2 = CenteredUV15_g2;
				float2 appendResult23_g2 = (float2(( length( CenteredUV15_g2 ) * 1.0 * 2.0 ) , ( atan2( break17_g2.x , break17_g2.y ) * ( 1.0 / TWO_PI ) * 1.0 )));
				float2 panner32 = ( 1.0 * _Time.y * _Speed + appendResult23_g2);
