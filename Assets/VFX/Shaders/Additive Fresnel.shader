Shader "VFX/Add+Fresnel"
{
	Properties
	{
		[HideInInspector] _AlphaCutoff("Alpha Cutoff ", Range(0, 1)) = 0.5
		[HideInInspector] _EmissionColor("Emission Color", Color) = (1,1,1,1)
		[ASEBegin]_TextureSample0("Texture Sample 0", 2D) = "white" {}
		_tiling("tiling", Vector) = (1,1,0,0)
		_tilingB("tilingB", Vector) = (1,1,0,0)
		_Speed("Speed", Vector) = (0.2,0,0,0)
		_SpeedB("SpeedB", Vector) = (0,0,0,0)
		[HDR]_Color0("Color 0", Color) = (0,0,0,0)
		[HDR]_Color1("Color 1", Color) = (1,1,1,0)
		_FresnelColor("Fresnel Color", Color) = (1,1,1,0)
		_FBias("F Bias", Float) = 0
		_FScale("F Scale", Float) = 1
		_FPower("F Power", Float) = 1
		_TextureSample1("Texture Sample 1", 2D) = "white" {}
